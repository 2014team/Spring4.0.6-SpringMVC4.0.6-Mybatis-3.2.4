<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper      
    PUBLIC "-//mybatis.apache.org//DTD Mapper 3.0//EN"      
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.user.dao.UserDao">

	<select id="select" parameterType="Map" resultType="com.ssm.user.entity.User">
		 SELECT * FROM users
	</select>
	
	<insert id="insert" parameterType="com.ssm.user.entity.User" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO USERS (name,passWord,comeFrom,createDate,updateDate)
	    values(#{name},#{passWord},#{comeFrom},NOW(),NOW())
	</insert>
	
</mapper>